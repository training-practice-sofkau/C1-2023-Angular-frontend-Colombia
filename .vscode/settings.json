{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.formatOnSave": false
  },
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.inlineSuggest.enabled": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#0b37ba",
    "activityBar.background": "#0b37ba",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#1e0209",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#0b37ba",
    "statusBar.background": "#08298a",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0b37ba",
    "statusBarItem.remoteBackground": "#08298a",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#08298a",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#08298a99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#08298A"
}
